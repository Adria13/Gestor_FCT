<!DOCTYPE html>

<html>
    <head>
        <meta charset="UTF-8" />
        <title>{% block title %}Welcome!{% endblock %}</title>
        <link rel="stylesheet" href="/css/style.css" media="screen" title="no title">
        <link rel="icon" type="image/x-icon" href="{{ asset('favicon.ico') }}" />
    </head>
    <body>
        {% block body %}
          <h1>Datos de la tabla:</h1>



<table border="1px" id="datos">


      <tr>
          <td>Nombre</td>
          <td>Dirección</td>
          <td>Código Postal</td>
          <td>Teléfono 1</td>
          <td>Teléfono 2</td>
          <td>Fecha de creación</td>
      </tr>


            {% for dato in datos %}
            <tr>
                <td id="tdTop" class="o">{{ dato.nombre }}</td>
                <td class="c">{{ dato.direccion }}</td>
                <td class="o">{{ dato.cp }}</td>
                <td class="c">{{ dato.telefono1 }}</td>
                <td class="o">{{ dato.telefono2 }}</td>
                <td id="tdBottom" class="c">{{ dato.fechaCreacion }}</td>
            </tr>
            {% endfor %}


</table>
  {% endblock %}
    </body>
</html>
